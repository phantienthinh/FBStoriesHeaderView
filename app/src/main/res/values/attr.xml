<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FBStoriesHeaderView">
        <attr name="size" format="integer" />
        <attr name="colorBackGround" format="color" />
        <attr name="colorSelect" format="color" />
        <attr name="tabHeight" format="dimension" />
        <attr name="tabSelect" format="integer" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBarView">
        <attr name="minValue" format="integer|dimension" />
        <attr name="maxValue" format="integer|dimension" />
        <attr name="currentValue" format="integer|dimension" />
        <attr name="barHeight" format="dimension" />
        <attr name="circleRadius" format="dimension" />
        <attr name="circleTextSize" format="dimension" />
        <attr name="baseColor" format="color" />
        <attr name="circleColor" format="color" />
        <attr name="rangerColor" format="color" />
        <attr name="rangerSize" format="integer" />
        <attr name="styleSeekbar" format="enum">
            <enum name="normal" value="1" />
            <enum name="color" value="2" />
            <enum name="ranger" value="3" />
        </attr>
    </declare-styleable>
</resources>